{
    "kernelSource": {
      "name": "frost-veux-kernel",
      "repo": "https://github.com/frost-testzone/kernel_xiaomi_sm6375",
      "branch": "main",
      "device": "veux",
      "defconfig": "vendor/veux-qgki_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "name": "android-clang",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "android-clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "LD": "android-clang/bin/ld.lld",
        "AR": "android-clang/bin/llvm-ar",
        "NM": "android-clang/bin/llvm-nm",
        "OBJCOPY": "android-clang/bin/llvm-objcopy",
        "OBJDUMP": "android-clang/bin/llvm-objdump",
        "READELF": "android-clang/bin/llvm-readelf",
        "OBJSIZE": "android-clang/bin/llvm-size",
        "STRIP": "android-clang/bin/llvm-strip"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/osm0sis/AnyKernel3",
      "branch": "master"
    }
}
